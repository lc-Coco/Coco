page {
	background-color: #dbdbdb;
}
#header {
	width: 100%;
	height: 100%;
	padding: 20rpx 30rpx;
	box-sizing: border-box;
	background-color: #fff;
}
.prompt {
	width: 100%;
	height: 80rpx;
	font-size: 30rpx;
	color: #ed3434;
	padding: 20rpx 30rpx;
}

#main {
	width: 100%;
	height: 100%;
	padding: 30rpx;
	font-size: 30rpx;
	position: relative;
	box-sizing: border-box;
}

.name {
	width: 100%;
	height: 80rpx;
	border-radius: 20rpx;
	background-color: #fff;
	padding-left: 100rpx;
	box-sizing: border-box;
}
.E-mail {
	width: 100%;
	height: 80rpx;
	border-radius: 20rpx;
	background-color: #fff;
	padding-left: 100rpx;
	box-sizing: border-box;
	margin-top: 20rpx;
}

textarea {
	width: 100%;
	margin-top: 20rpx;
	padding: 10rpx;
	border-radius: 12rpx;
	box-sizing: border-box;
	background-color: #fff;
}
.nameText {
	position: absolute;
	left: 46rpx;
	top: 48rpx;
}
.E-mailText {
	position: absolute;
	left: 46rpx;
	top: 148rpx;
}

.btn {
	width: 100%;
	height: 80rpx;
    margin-top: 40rpx;
    color: white;
    background-color: #ed3434;
    border-radius: 12rpx;
}

